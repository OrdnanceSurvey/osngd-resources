{
    "style-options": {
        "fill": {
            "fill-color": "#000000",
            "fill-opacity": 0.3,
            "fill-outline-color": "#000000",
            "fill-outline-opacity": 1.0,
            "fill-outline-width": 1
        },
        "line": {
            "line-color": "#000000",
            "line-width": 1,
            "*line-cap": "butt",
            "*line-dasharray": [ 2, 2 ],
            "line-opacity": 1.0
        },
        "circle": {
            "circle-radius": 4,
            "circle-color": "#000000",
            "circle-opacity": 1.0,
            "circle-stroke-color": "#ffffff",
            "circle-stroke-opacity": 0.9,
            "circle-stroke-width": 1
        },
        "*symbol": {
            "icon-image": "marker.svg",
            "icon-anchor": [ 0, 0 ],
            "icon-size": [ 24, 24 ],
            "icon-rotate": 0,
            "icon-opacity": 1.0
        }
    },
    "color-options": {
        "string": "#000000",
        "array": [
            "description",
            "val1", "#000000",
            "val2", "#000000",
            "#000000"
        ],
        "pattern": "data:image/png;base64,..."
    },
    "themes": {
        "add": {
            "name": "Address",
            "feature-types": {
                "add-gb-builtaddress": "circle",
                "add-gb-historicaddress": "circle",
                "add-gb-nonaddressableobject": "circle",
                "add-gb-prebuildaddress": "circle",
                "add-gb-streetaddress": "circle",
                "add-isl-builtaddress": "circle",
                "add-isl-historicaddress": "circle",
                "add-isl-nonaddressableobject": "circle",
                "add-isl-prebuildaddress": "circle",
                "add-isl-streetaddress": "circle"
            },
            "main-color": "#dc5b6e",
            "collection-colors": {
                "gb": "#dc5b6e",
                "isl": "#e06c7d"
            }
        },
        "asu": {
            "name": "Administrative and Statistical Units",
            "feature-types": {
                "asu-bdy-boundaryhighwatermark": "line",
                "asu-bdy-ceremonialcounty": "fill",
                "asu-bdy-country": "fill",
                "asu-bdy-pollingdistrict": "fill",
                "asu-bdy-devolvedparliamentconstituency": "fill",
                "asu-bdy-devolvedparliamentelectoralregion": "fill",
                "asu-bdy-electoraldivision": "fill",
                "asu-bdy-ward": "fill",
                "asu-bdy-westminsterconstituency": "fill",
                "asu-bdy-glaassemblyconstituency": "fill",
                "asu-bdy-lowertierlocalauthority": "fill",
                "asu-bdy-parishorcommunity": "fill",
                "asu-bdy-uppertierlocalauthority": "fill",
                "asu-bdy-historiccounty": "fill",
                "asu-bdy-historiceuropeanregion": "fill",
                "asu-bdy-region": "fill",
                "asu-bdy-regionalauthority": "fill"
            },
            "main-color": "#6a54b4",
            "collection-colors": {
                "bdy": "#6a54b4"
            }
        },
        "bld": {
            "name": "Buildings",
            "feature-types": {
                "bld-fts-buildingline": "line",
                "bld-fts-buildingpart": "fill"
            },
            "main-color": "#ead04b",
            "collection-colors": {
                "fts": "#ead04b"
            },
            "contextual-colors": {
                "bld-fts-buildingpart": "#dcd7c6"
            }
        },
        "gnm": {
            "name": "Geographical Names",
            "feature-types": {
                "gnm-fts-namedarea": "fill",
                "gnm-fts-namedpoint": "circle"
            },
            "main-color": "#857b82",
            "collection-colors": {
                "fts": "#857b82"
            }
        },
        "lnd": {
            "name": "Land",
            "feature-types": {
                "lnd-fts-land": "fill",
                "lnd-fts-landpoint": "circle",
                "lnd-fts-landform": "fill",
                "lnd-fts-landformline": "line",
                "lnd-fts-landformpoint": "circle"
            },
            "main-color": "#55a973",
            "collection-colors": {
                "fts": "#55a973"
            },
            "contextual-colors": {
                "lnd-fts-land": [
                    "oslandcovertiera",
                    "Excavated Or Deposited", "#eaeae4",
                    "Made", "#f2f2e9",
                    "Mineral", "#eaead3",
                    "Multiple", "#eeefda",
                    "Open Vegetation", "#e4efda",
                    "Open Vegetation And Mineral", "#e4efda",
                    "Trees", "#cee6bd",
                    "Under Construction", "#f8f6f0",
                    "Water", "#aadeef",
                    "#000000"
                ],
                "lnd-fts-landform": "data:image/png;base64,"
            }
        },
        "lus": {
            "name": "Land Use",
            "feature-types": {
                "lus-fts-site": "fill",
                "lus-fts-siteaccesslocation": "circle",
                "lus-fts-siteroutingpoint": "circle"
            },
            "main-color": "#b38b6d",
            "collection-colors": {
                "fts": "#b38b6d"
            }
        },
        "str": {
            "name": "Structures",
            "feature-types": {
                "str-fts-compoundstructure": "fill",
                "str-fts-structure": "fill",
                "str-fts-structureline": "line",
                "str-fts-structurepoint": "circle"
            },
            "main-color": "#edb5b5",
            "collection-colors": {
                "fts": "#edb5b5"
            },
            "contextual-colors": {
                "str-fts-structure": "#e7c9c8"
            }
        },
        "trn": {
            "name": "Transport",
            "feature-types": {
                "trn-fts-cartographicraildetail": "line",
                "trn-fts-rail": "fill",
                "trn-fts-roadline": "line",
                "trn-fts-roadtrackorpath": "fill",
                "trn-ntwk-connectinglink": "line",
                "trn-ntwk-connectingnode": "circle",
                "trn-ntwk-path": "line",
                "trn-ntwk-pathlink": "line",
                "trn-ntwk-pathnode": "circle",
                "trn-ntwk-ferrylink": "line",
                "trn-ntwk-ferrynode": "circle",
                "trn-ntwk-ferryterminal": "circle",
                "trn-ntwk-road": "line",
                "trn-ntwk-roadjunction": "circle",
                "trn-ntwk-roadlink": "line",
                "trn-ntwk-roadnode": "circle",
                "trn-ntwk-street": "line",
                "trn-rami-highwaydedication": "line",
                "trn-rami-restriction": "line",
                "trn-rami-routinghazard": "line",
                "trn-rami-routingstructure": "line",
                "trn-rami-maintenancearea": "fill",
                "trn-rami-maintenanceline": "line",
                "trn-rami-maintenancepoint": "circle",
                "trn-rami-reinstatementarea": "fill",
                "trn-rami-reinstatementline": "line",
                "trn-rami-reinstatementpoint": "circle",
                "trn-rami-specialdesignationarea": "fill",
                "trn-rami-specialdesignationline": "line",
                "trn-rami-specialdesignationpoint": "circle"
            },
            "main-color": "#f19748",
            "collection-colors": {
                "fts": "#f19748",
                "ntwk": "#f6bc89",
                "rami": "#a6540d"
            },
            "contextual-colors": {
                "trn-fts-rail": "#cccbcb",
                "trn-fts-roadtrackorpath": [
                    "description",
                    "Central Reservation", "#cccbcb",
                    "Cycle Way", "#fcfdff",
                    "Escape Lane", "#fcfdff",
                    "Ford", "#fcfdff",
                    "Lay-by", "#fcfdff",
                    "Level Crossing", "#fcfdff",
                    "Path", "#dcdcdb",
                    "Path And Steps", "#dcdcdb",
                    "Path On Pipe", "#dcdcdb",
                    "Pavement", "#cccbcb",
                    "Pavement And Steps", "#cccbcb",
                    "Pedestrian Crossing", "#fcfdff",
                    "Road", "#fcfdff",
                    "Road Turntable", "#fcfdff",
                    "Roofed Path", "#dcdcdb",
                    "Roofed Track", "#dcdcdb",
                    "Sand Drag", "#f4f0d3",
                    "Shared Use Carriageway", "#fcfdff",
                    "Towing Path", "#fcfdff",
                    "Track", "#dcdcdb",
                    "Traffic Calming", "#fcfdff",
                    "Transport Curtilage ", "#cccbcb",
                    "Travelling Walkway", "#dcdcdb",
                    "Vehicle Dip", "#fcfdff",
                    "#000000"
                ]
            }
        },
        "wtr": {
            "name": "Water",
            "feature-types": {
                "wtr-fts-intertidalline": "line",
                "wtr-fts-tidalboundary": "line",
                "wtr-fts-water": "fill",
                "wtr-fts-waterpoint": "circle",
                "wtr-ntwk-waterlink": "line",
                "wtr-ntwk-waternode": "circle",
                "wtr-ntwk-waterlinkset": "line"
            },
            "main-color": "#2d8fb6",
            "collection-colors": {
                "fts": "#2d8fb6",
                "ntwk": "#99d0e6"
            },
            "contextual-colors": {
                "wtr-fts-water": "#aadeef"
            }
        }
    }
}
